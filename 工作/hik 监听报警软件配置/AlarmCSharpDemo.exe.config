<?xml version="1.0" encoding="utf-8"?>
<!-- C# 闹钟演示应用程序配置文件 -->
<configuration>
  <!-- 启动配置部分 -->
  <startup>
    <!-- 指定支持的.NET Framework运行时版本 -->
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  
  <!-- 应用程序设置部分 -->
  <appSettings>
    <!-- 闹钟提醒间隔时间（单位：分钟） -->
    <add key="interval" value="" />
    
    <!-- 闹钟重复提醒次数 -->
    <add key="times" value="" />
    
    <!-- 闹钟音乐文件路径（使用相对路径，文件位于程序根目录下的 sounds 文件夹中） -->
    <add key="musicPath" value="sounds\W8cengya.wav" />
    
    <!-- 是否显示弹窗提醒（true=显示，false=不显示） -->
    <add key="popWindow" value="false" />
    
    <!-- 超脑IP地址 -->
    <add key="ip" value="10.30.5.186" /> 
    
    <!-- 超脑端口号 -->
    <add key="port" value="8000" />
    
    <!-- 超脑登录用户名 -->
    <add key="username" value="admin" />
    
    <!-- 超脑登录密码 -->
    <add key="password" value="wdzn6688" />
    
    <!-- 超脑通道号（留空表示使用默认通道） -->
    <add key="channelId" value="42" />
    
    <!-- AI模型ID，用于智能识别功能 -->
    <add key="modelId" value="6beef5384db946d487c8bd146e4bfdc7" />
    
    <!-- 任务ID（留空表示使用默认任务） -->
    <add key="taskId" value="" />
    
    <!-- 客户端设置提供者服务URI（留空表示使用本地设置） -->
    <add key="ClientSettingsProvider.ServiceUri" value="" />
  </appSettings>
  
  <!-- 系统Web服务配置部分 -->
  <system.web>
    <!-- 成员身份验证提供者配置 -->
    <membership defaultProvider="ClientAuthenticationMembershipProvider">
      <providers>
        <!-- 客户端表单身份验证成员身份提供者 -->
        <add name="ClientAuthenticationMembershipProvider" type="System.Web.ClientServices.Providers.ClientFormsAuthenticationMembershipProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" />
      </providers>
    </membership>
    
    <!-- 角色管理器配置 -->
    <roleManager defaultProvider="ClientRoleProvider" enabled="true">
      <providers>
        <!-- 客户端角色提供者，缓存超时时间设置为86400秒（24小时） -->
        <add name="ClientRoleProvider" type="System.Web.ClientServices.Providers.ClientRoleProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" cacheTimeout="86400" />
      </providers>
    </roleManager>
  </system.web>
</configuration>